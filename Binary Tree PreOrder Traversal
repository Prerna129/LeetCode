class Solution {
public:
    vector<int>ans;
   
    void preorder(TreeNode*root){
    if(root==NULL){
        return;
    }
    ans.push_back(root->val);
    cout<<root->val<<" ";
    preorder(root->left);
    preorder(root->right);

 }
  vector<int> preorderTraversal(TreeNode* root) {
       preorder(root);

       return ans;
    }
};
