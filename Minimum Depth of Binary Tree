
class Solution {
public:
    int minDepth(TreeNode* root) {

        if(root==NULL){
            return NULL;
        }
        int left=minDepth(root->left);
        int right=minDepth(root->right);
        
        if(left==0 || right==0){
            return(1+right+left);
        }

        else{
            return min(left,right)+1;
        }
    }
};
